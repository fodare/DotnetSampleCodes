@model IEnumerable<User>
@{
}
<h1>Users list</h1>
<hr />

<table class="usertable">
    <tr>
        <th>Username</th>
        <th>Password</th>
        <th>Email</th>
        <th>Role</th>
       
    </tr>
    @foreach (var u in Model)
    {
        <tr>
            <td>@u.UserName</td>
            <td>@u.password</td>
            <td>@u.Email</td>
            <td>@u.Role</td>
        </tr>
    }
</table>
