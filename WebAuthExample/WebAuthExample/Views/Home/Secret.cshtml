@{
    ViewData["Title"] = "Secret";
}
@model IEnumerable<Secret>
<h1>Secrets</h1>

<p>Below are some of the world secrets:</p>
<table class="secretTable">
    <tr>
       <th>Id</th>
       <th>Name</th>
       <th>Secret</th>
       <th>Created on</th>
       <th>Creator</th>
    </tr>
    @foreach(var s in Model)
    {
        <tr>
            <td>@s.Id</td>
            <td>@s.SercretName</td>
            <td>@s.SercretValue</td>
            <td>@s.CreatedDate</td>
            <td>@s.Creator</td>
        </tr>
    }
</table>